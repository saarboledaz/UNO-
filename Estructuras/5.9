import java.util.HashMap;
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner e = new Scanner(System.in);
        HashMap<String, String> hm = new HashMap<>();
        String[] palabras;
        int np = e.nextInt();
        String p,p_aux;
        palabras = new String[np];
        for (int i = 0;i<np;i++){
            p = e.next();
            hm.put(p,p);
            palabras[i]=p;
        }
        for (int i = 0;i<np;i++){
            p="";
            for (int j = 0;j<palabras[i].length();j++){
                p=p+palabras[i].charAt(j);
                if (!p.equals(palabras[i])&&hm.containsKey(p)){
                    p_aux=palabras[i].substring(j+1,palabras[i].length());
                    if (hm.containsKey(p_aux)){
                        System.out.println(palabras[i]+" = "+p+" + "+p_aux);
                    }
                }
            }
        }
    }
}
